[
    {
        "kernelSource": {
            "name": "kali",
            "repo": "https://github.com/OutlinedArc217/kernel_oppo_a33f",
            "branch": "main",
            "device": "a33f",
            "defconfig": "a33f_defconfig"
        },
        "toolchains": [
            {
                "name": "aarch64-linux-android",
                "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-10.0.0_r32.tar.gz",
                "binaryEnv": ["bin"]
            }
        ],
        "enableCcache": false,
        "params": {
            "ARCH": "arm64",
            "externalCommands": {
                "CROSS_COMPILE": "aarch64-linux-android-"
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true
        },
        "enableKernelSU": false,
        "enableLXC": true
    }
]
